<StackPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FMS">
  <children>
    <Pane prefHeight="695.0" prefWidth="957.0">
      <children>
        <MenuButton fx:id="recordmenu" layoutX="52.0" layoutY="149.0" mnemonicParsing="false" style="-fx-background-radius: 10;" text="Filter Record">
          <items>
            <MenuItem mnemonicParsing="false" text="Open Record" />
            <MenuItem mnemonicParsing="false" text="Closed Record" />
            <MenuItem mnemonicParsing="false" text="Unassigned Record" />
            <MenuItem mnemonicParsing="false" text="All Records" />
          </items>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
        </MenuButton>
        <TabPane layoutX="212.0" layoutY="149.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="554.0" prefWidth="1052.0" side="LEFT" tabClosingPolicy="UNAVAILABLE">
          <tabs>
            <Tab closable="false" text="Records">
              <content>
                <TableView fx:id="recordtable" editable="true" onMouseMoved="#filltable" prefHeight="530.0" prefWidth="977.0" tableMenuButtonVisible="true">
                  <columns>
                    <TableColumn fx:id="id" prefWidth="10.0" text="ID" />
                    <TableColumn fx:id="status" prefWidth="60.0" text="Status" />
                    <TableColumn fx:id="workerid" prefWidth="50.0" text="Worker ID" />
                    <TableColumn fx:id="studentid" prefWidth="50.0" text="Student ID" />
                    <TableColumn fx:id="requesttype" prefWidth="75.0" text="Request Type" />
                    <TableColumn fx:id="starttime" prefWidth="75.0" text="Request Time" />
                    <TableColumn fx:id="closedtime" prefWidth="75.0" text="Close Time" />
                    <TableColumn fx:id="roomnum" prefWidth="50.0" text="Room No." />
                    <TableColumn fx:id="hostel" prefWidth="60.0" text="Hostel" />
                    <TableColumn fx:id="comment" prefWidth="10.0" text="Comment" />
                  </columns>
                  <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                  </columnResizePolicy>
                  <cursor>
                    <Cursor fx:constant="HAND" />
                  </cursor>
                </TableView>
              </content>
            </Tab>
          </tabs>
          <effect>
            <DropShadow />
          </effect>
        </TabPane>
        <Label alignment="CENTER" layoutX="481.0" layoutY="32.0" prefHeight="34.0" prefWidth="510.0" text="Records Table">
          <font>
            <Font name="Oswald Regular" size="45.0" />
          </font>
        </Label>
        <JFXButton layoutX="35.0" layoutY="204.0" onAction="#addemployee" ripplerFill="#46de02" style="-fx-background-color: #f79a05; -fx-background-radius: 50;" text="Employee Details">
               <font>
                  <Font name="Oswald Regular" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/details.png" />
                     </image>
                  </ImageView>
               </graphic></JFXButton>
        <JFXButton layoutX="99.0" layoutY="48.0" onAction="#login" ripplerFill="#e04d02" style="-fx-background-color: #f7059a; -fx-background-radius: 50;" text="Logout">
               <font>
                  <Font name="Oswald Regular" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/logout%20.png" />
                     </image>
                  </ImageView>
               </graphic></JFXButton>
      </children>
    </Pane>
  </children>
